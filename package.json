{
  "name": "script-lang",
  "version": "1.0.0",
  "description": "ScriptLang project scaffold and implementation",
  "main": "dist/index.js",
  "bin": {
    "scriptlang-player": "./dist/src/cli/player.js"
  },
  "scripts": {
    "validate:docs": "node scripts/validate-docs.mjs",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "player:dev": "tsx src/cli/player.ts",
    "player:tui": "node dist/src/cli/player.js tui",
    "player:agent": "node dist/src/cli/player.js agent",
    "test:unit": "vitest run",
    "coverage:strict": "vitest run --coverage",
    "quality:gate": "npm run validate:docs && npm run lint && npm run typecheck && npm run coverage:strict",
    "pretest": "npm run quality:gate",
    "test": "npm run test:unit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^10.0.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "ink": "^6.8.0",
    "react": "^19.2.0",
    "react-devtools-core": "^7.0.1",
    "saxes": "^6.0.0"
  }
}

{
  "name": "script-lang",
  "version": "1.0.0",
  "description": "ScriptLang project scaffold and implementation",
  "main": "dist/index.js",
  "scripts": {
    "validate:docs": "node scripts/validate-docs.mjs",
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "test:unit": "vitest run",
    "coverage:strict": "vitest run --coverage",
    "quality:gate": "npm run validate:docs && npm run typecheck && npm run coverage:strict",
    "pretest": "npm run quality:gate",
    "test": "npm run test:unit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@types/node": "^25.3.0",
    "@vitest/coverage-v8": "^4.0.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "saxes": "^6.0.0"
  }
}
